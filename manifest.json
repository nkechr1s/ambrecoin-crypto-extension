{
  "manifest_version": 3,
  "name": "Ambrecoin - Crypto Prices",
  "description": "Discover prices of crypto faster than ever!",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "activeTab",
    "contextMenus",
    "storage",
    "https://*.google.com/"
  ],
  "host_permissions": [
    "https://api.coingecko.com/"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "window.html"
  }
}
